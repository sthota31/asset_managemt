
   <div class="form-group">
    <%= f.label :name,:class=>"col-sm-2 control-label" %>
    <div class="col-sm-4">
      <%= f.text_field :name,:class=>"form-control" %>
    </div>
  </div>

  <div class="form-group">
    <%= f.label :asset_type,:class=>"col-sm-2 control-label" %>
    <div class="col-sm-4">
      <%= f.select :asset_type, ['Desktop','Laptop'],:class=>"form-control" %>
    </div>
  </div>

  <div class="form-group">
    <%= f.label :make,:class=>"col-sm-2 control-label"  %>
    <div class="col-sm-4">
     <%= f.text_field :make,:class=>"form-control", :required => true%>
    </div>
  </div>

  <div class="form-group">
    <%= f.label :model ,:class=>"col-sm-2 control-label"  %>
    <div class="col-sm-4">
    <%= f.text_field :model,:class=>"form-control" , :required => true%>
  </div>
  </div>

  <div class="form-group">
    <%= f.label :purchase_date ,:class=>"col-sm-2 control-label"  %>
    <div class="col-sm-4">
    <%= f.text_field :purchase_date,:class=>"form-control datepicker" %>
   </div>
  </div>

  <div class="form-group">
    <%= f.label :warranty_date,:class=>"col-sm-2 control-label" %>
    <div class="col-sm-4">
      <%= f.text_field :warranty_date,:class=>"form-control datepicker" %>
    </div>
  </div>

  <div class="form-group">
    <%= f.label :vendor_name,:class=>"col-sm-2 control-label" %>
    <div class="col-sm-4">
      <%= f.text_field :vendor_name,:class=>"form-control" %>
    </div>
  </div>

  <div class="form-group">
    <%= f.label "AssetId",:class=>"col-sm-2 control-label" %>
    <div class="col-sm-4">
      <%= f.text_field :uniqasset_id,:class=>"form-control" %>
    </div>
  </div>

<div class="form-group">
    <div class="col-sm-offset-2 col-sm-10">
     <button type="submit" class="btn btn-sm">Submit</button>
  </div>
</div>





